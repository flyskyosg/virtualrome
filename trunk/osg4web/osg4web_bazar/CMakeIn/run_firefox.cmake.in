
#cmake script for batch building
  FILE(TO_CMAKE_PATH "$ENV{PATH}" ACTUAL_PATH)
  SET(ACTUAL_PATH ${ACTUAL_PATH} "@OSG_BIN_DIR@") 
  FILE(TO_NATIVE_PATH "${ACTUAL_PATH}" ACTUAL_NATIVE_PATH)
  SET(ENV{PATH} "${ACTUAL_NATIVE_PATH}")
