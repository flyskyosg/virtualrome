  IF(NOT ADD_FIREFOX_ARGS)
        SET(ADD_FIREFOX_ARGS " -no-remote -P osg4web_test_profile")
  ENDIF(NOT ADD_FIREFOX_ARGS)

  EXEC_PROGRAM(
     "@FIREFOX_EXE@" 
     "@CMAKE_BINARY_DIR@"
     ARGS ${URL} ${ADD_FIREFOX_ARGS}
   )
