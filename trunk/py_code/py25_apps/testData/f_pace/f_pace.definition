connect  f_pace_mid   f_pace_portico_tetto_mid                f_pace_low   f_pace_portico_low 
connect  f_pace_mid   f_pace_portico_architrave_mid           f_pace_low   f_pace_portico_low 

connect  f_pace_hi    f_pace_muro_hi                          f_pace_mid   f_pace_muro_mid
connect  f_pace_hi    h_f_pace_pavimento                      f_pace_mid   f_pace_pavimento_mid
connect  f_pace_hi    f_pace_pavimento_decorazioni_altare     f_pace_mid   f_pace_pavimento_decorazioni_mid 
connect  f_pace_hi    f_pace_pavimento_decorazioni_basi*      f_pace_mid   f_pace_pavimento_decorazioni_mid 
